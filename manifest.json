
{
  "manifest_version": 2,
  "name": "Netflix Tracker",
  "version": "0.1",

  "browser_action": {
    "default_icon": "res/icon.png"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["src/content.js"]
    }
  ],

  "background": {
    "scripts": ["src/background.js"]
  }
}